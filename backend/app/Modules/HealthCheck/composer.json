{
    "name": "modules/healthcheck",
    "description": "HealthCheck Module - Self-contained health check module with monitoring and logging capabilities",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "FetchIt Team",
            "email": "dev@customerportal.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0",
        "illuminate/routing": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Modules\\HealthCheck\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Modules\\HealthCheck\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Modules\\HealthCheck\\Providers\\HealthCheckServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
